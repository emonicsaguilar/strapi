{
  "kind": "collectionType",
  "collectionName": "movies",
  "info": {
    "singularName": "movie",
    "pluralName": "movies",
    "displayName": "movies",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "rating": {
      "type": "integer",
      "default": 0,
      "min": 0,
      "max": 5,
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "readOnly": {
      "type": "boolean",
      "default": true,
      "required": false
    },
    "popular": {
      "type": "boolean",
      "default": false,
      "required": false
    },
    "reviews": {
      "type": "component",
      "repeatable": true,
      "component": "basic.review"
    }
  }
}
